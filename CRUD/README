CREATE PROJECT 
composer create-project laravel/laravel projectname 

CREATE MODEL
php artisan make:model Product -m

MAKE CHANGES IN .ENV FILE
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=laravel
DB_USERNAME=student
DB_PASSWORD=student

CREATE CONTROLLER WITH FUNCION
php artisan make:controller Controllername --resource

MIGRATE TABLES
php artisan migrate
